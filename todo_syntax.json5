[
  {
    "language": "Groovy",
    "extensions": [
      ".groovy",
      "Jenkinsfile",
      ".Jenkinsfile"
    ],
    "markers": [
      {
        "type": "line",
        "pattern": "//"
      },
      {
        "type": "block",
        "pattern": {
          "start": "/\\*",
          "end": "\\*/"
        }
      }
    ]
  }
]